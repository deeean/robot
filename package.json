{
  "name": "@deeean/sophia",
  "version": "0.0.7",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "deeean <margintop3498@icloud.com>",
  "license": "MIT",
  "keywords": [
    "AutoHotkey",
    "RobotJS",
    "AutoIt",
    "Automation"
  ],
  "repository": "https://github.com/deeean/sophia.git",
  "bugs": {
    "url": "https://github.com/deeean/sophia/issues"
  },
  "homepage": "https://github.com/deeean/sophia#readme",
  "watch": {
    "build-rs": {
      "patterns": [
        "src"
      ],
      "extensions": "rs",
      "quiet": true
    },
    "run-ts": {
      "patterns": [
        "example",
        "*.node"
      ],
      "extensions": "ts",
      "quiet": true
    }
  },
  "scripts": {
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "test": "ava",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.11.4",
    "ava": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "napi": {
    "name": "sophia",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "optionalDependencies": {
    "@deeean/sophia-win32-x64-msvc": "0.0.7"
  }
}